class character_creation():
    name: str
    race: str
    age: str
    selectedclass: str
def character_creation():
    name = input("Enter your name:")
    race = input("Enter your last name:")
    age = input("Age:")
    selectedclass = input("Enter your address:") 
    return name, race, age, selectedclass

